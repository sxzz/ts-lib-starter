{
  "name": "node-lib-starter",
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.1",
  "description": "My awesome typescript library",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/node-lib-starter#readme",
  "bugs": {
    "url": "https://github.com/sxzz/node-lib-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/node-lib-starter.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json,.md",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsup && tsx scripts/postbuild.mts",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^2.4.6",
    "@sxzz/eslint-config-ts": "^2.4.6",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "eslint-define-config": "^1.17.0",
    "fast-glob": "^3.2.12",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
